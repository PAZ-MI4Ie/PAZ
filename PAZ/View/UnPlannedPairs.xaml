<Window x:Class="PAZ.View.UnPlannedPairs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Nog te plannen" Height="696" Width="279" Icon="/PAZ;component/Resources/paz.ico">
    <Grid Background="#FFD51D53" DataContext="{Binding}" ShowGridLines="False" ClipToBounds="True">
        <Grid.RowDefinitions>
            <RowDefinition Height="10" />
            <RowDefinition />
        </Grid.RowDefinitions>
        <ScrollViewer Grid.Row="1" VerticalScrollBarVisibility="Auto">
        <Grid Name="GridPairs"  Grid.Row="1" Background="White" Margin="5,0,5,5"></Grid>
        </ScrollViewer>
    </Grid>
</Window>
